<?xml version="1.0" encoding="UTF-8"?>
<EclipseWork>
    <wizard>
        <title>DataOutputStreamResult Result (J2ME)</title>

        <component-page required="true">
            <description>Use this result to create J2ME HTTP clients</description>

				<textfield name="name" label="Name:" required="true" value="J2MEResult" />
